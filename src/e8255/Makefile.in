srcdir := @srcdir@
topdir := @top_srcdir@
subdir :=
reldir := ../..

include $(reldir)/Makefile.inc


SRC	:= e8255.c

OBJ	:= e8255.o

HDR	:= e8255.h
HDR_EXT	:=

MAN	:=

BIN	:= e8255.a

SDP	:= Makefile $(HDR) $(HDR_EXT)
BDP	:= Makefile $(OBJ) $(OBJ_EXT)


all: $(BIN)


clean:
	rm -f $(BIN) $(OBJ)


e8255.a: $(OBJ)
	rm -f $@
	$(AR) $(AR_FLG) $@ $(OBJ)


e8255.o:	e8255.c $(SDP)
