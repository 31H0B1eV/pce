# Makefile
# $Id: Makefile.in,v 1.14 2003/11/08 14:20:20 hampa Exp $

srcdir := @srcdir@
topdir := @top_srcdir@
subdir := lib libini terminal chipset e8086 ibmpc
reldir := ..

ifeq "@PCE_HAVE_NASM@" "1"
subdir += pceutils
endif


include $(reldir)/Makefile.inc


SRC	:=
OBJ	:=
HDR	:=
MAN	:=
BIN	:=

SDP	:= Makefile $(HDR)
BDP	:= Makefile $(OBJ)


all: $(BIN) $(OBJ)


clean:
	rm -f $(OBJ) $(BIN)
