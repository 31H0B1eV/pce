# Makefile
# $Id: Makefile.in,v 1.3 2003/11/16 03:44:26 hampa Exp $

srcdir := @srcdir@
topdir := @top_srcdir@
subdir :=
reldir := ../..

include $(reldir)/Makefile.inc


FILES := cmd hexdump log

SRC	:= $(foreach f,$(FILES),$(f).c)

OBJ	:= $(foreach f,$(FILES),$(f).o)

HDR	:= $(foreach f,$(FILES),$(f).h)

BIN	:=

SDP	:= Makefile
BDP	:= Makefile $(OBJ)


all: $(OBJ)


clean:
	rm -f $(BIN) $(OBJ)


cmd.o:		cmd.c cmd.h $(SDP)
hexdump.o:	hexdump.c hexdump.h $(SDP)
log.o:		log.c log.h $(SDP)
