# Makefile
# $Id: Makefile.in,v 1.2 2003/11/08 14:40:34 hampa Exp $

srcdir := @srcdir@
topdir := @top_srcdir@
subdir :=
reldir := ../..

include $(reldir)/Makefile.inc


SRC	:= cmd.c log.c

OBJ	:= cmd.o log.o

HDR	:= cmd.h log.h
HDR_EXT	:=

MAN	:=

BIN	:=

SDP	:= Makefile
BDP	:= Makefile $(OBJ) $(OBJ_EXT)


all: $(OBJ)


clean:
	rm -f $(BIN) $(OBJ)


cmd.o:	cmd.c cmd.h $(SDP)
log.o:	log.c log.h $(SDP)
