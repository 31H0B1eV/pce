# Makefile
# $Id$

srcdir := @srcdir@
topdir := @top_srcdir@
subdir :=
reldir := ../../..


include $(reldir)/config.inc


SRC := disasm.c e6502.c ea.c opcodes.c
OBJ := disasm.o e6502.o ea.o opcodes.o
HDR := e6502.h internal.h
MAN :=
BIN := e6502.a

SDP := Makefile $(HDR) $(HDR_EXT)
BDP := Makefile $(OBJ) $(OBJ_EXT)

CLN := $(OBJ) $(BIN)
DCL := Makefile


include $(reldir)/rules.inc


all: $(BIN)


e6502.a: $(OBJ)
	rm -f $@
	$(AR) $(AR_FLG) $@ $(OBJ)


disasm.o:	disasm.c $(SDP)
e6502.o:	e6502.c $(SDP)
ea.o:		ea.c $(SDP)
opcodes.o:	opcodes.c $(SDP)
