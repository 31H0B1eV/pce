# Makefile
# $Id$

srcdir := @srcdir@
topdir := @top_srcdir@
subdir :=
reldir := ../..


include $(reldir)/config.inc


ifeq "$(PCE_BUILD_6502)" "1"
subdir += e6502
endif

ifeq "$(PCE_BUILD_68000)" "1"
subdir += e68000
endif

ifeq "$(PCE_BUILD_8086)" "1"
subdir += e8086
endif

ifeq "$(PCE_BUILD_ARM)" "1"
subdir += arm
endif

ifeq "$(PCE_BUILD_PPC405)" "1"
subdir += ppc405
endif

ifeq "$(PCE_BUILD_SPARC32)" "1"
subdir += sparc32
endif


DCL := Makefile

DIST := Makefile.in


include $(reldir)/rules.inc
