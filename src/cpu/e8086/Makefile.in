# Makefile
# $Id: Makefile.in,v 1.2 2003/12/29 11:06:23 hampa Exp $

srcdir := @srcdir@
topdir := @top_srcdir@
subdir :=
reldir := ../../..


include $(reldir)/config.inc


SRC := disasm.c e8086.c e80186.c e80286r.c flags.c ea.c opcodes.c pqueue.c
OBJ := disasm.o e8086.o e80186.o e80286r.o flags.o ea.o opcodes.o pqueue.o
HDR := e8086.h internal.h
MAN :=
BIN := e8086.a

SDP := Makefile $(HDR) $(HDR_EXT)
BDP := Makefile $(OBJ) $(OBJ_EXT)

CLN := $(OBJ) $(BIN)
DCL := Makefile


include $(reldir)/rules.inc


all: $(BIN)


e8086.a: $(OBJ)
	rm -f $@
	$(AR) $(AR_FLG) $@ $(OBJ)


disasm.o:	disasm.c $(SDP)
e8086.o:	e8086.c $(SDP)
e80186.o:	e80186.c $(SDP)
e80286r.o:	e80286r.c $(SDP)
ea.o:		ea.c $(SDP)
flags.o:	flags.c $(SDP)
opcodes.o:	opcodes.c $(SDP)
pqueue.o:	pqueue.c $(SDP)
