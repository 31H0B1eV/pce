DIST := $(SRC) $(HDR) Makefile.in


include $(reldir)/rules.inc


all: $(BIN)


libini.a: $(OBJ)


read.o:		read.c $(SDP)
scanner.o:	scanner.c $(SDP)
section.o:	section.c $(SDP)
value.o:	value.c $(SDP)
write.o:	write.c $(SDP)
