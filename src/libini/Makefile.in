# Makefile
# $Id: Makefile.in,v 1.2 2003/12/29 11:06:23 hampa Exp $

srcdir := @srcdir@
topdir := @top_srcdir@
subdir :=
reldir := ../..


include $(reldir)/config.inc


SRC := read.c scanner.c section.c value.c write.c
OBJ := read.o scanner.o section.o value.o write.o
HDR := libini.h
MAN :=
BIN := libini.a

SDP := Makefile $(HDR) scanner.h
BDP := Makefile $(OBJ)

CLN := $(OBJ) $(BIN)
DCL := Makefile


include $(reldir)/rules.inc


all: $(BIN)


libini.a: $(OBJ)
	rm -f $@
	$(AR) $(AR_FLG) $@ $(OBJ)


read.o:		read.c $(SDP)
scanner.o:	scanner.c $(SDP)
section.o:	section.c $(SDP)
value.o:	value.c $(SDP)
write.o:	write.c $(SDP)
