srcdir := @srcdir@
topdir := @top_srcdir@
subdir :=
reldir := ../..

include $(reldir)/Makefile.inc


SRC	:= e8259.c

OBJ	:= e8259.o

HDR	:= e8259.h
HDR_EXT	:=

MAN	:=

