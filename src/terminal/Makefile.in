# Makefile
# $Id$

srcdir := @srcdir@
topdir := @top_srcdir@
subdir :=
reldir := ../..


include $(reldir)/config.inc


SRC := font.c keys.c null.c scrmap.c terminal.c
OBJ := font.o keys.o null.o scrmap.o terminal.o
HDR := font.h keys.h null.h scrmap.h terminal.h
BIN := terminal.a

HDR_EXT := libini/libini.h

SDP := Makefile $(HDR) $(HDR_EXT)
BDP := Makefile $(OBJ) $(OBJ_EXT)

CLN := $(OBJ) $(BIN)
DCL := Makefile


include $(reldir)/rules.inc


all: $(BIN)


terminal.a: $(OBJ)


font.o:		font.c $(SDP)
keys.o:		keys.c $(SDP)
null.o:		null.c $(SDP)
scrmap.o:	scrmap.c $(SDP)
terminal.o:	terminal.c $(SDP)
