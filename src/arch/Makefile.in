# src/arch/Makefile

srcdir := @srcdir@
topdir := @top_srcdir@
subdir :=
reldir := ../..


include $(reldir)/config.inc


ifeq "$(PCE_BUILD_IBMPC)" "1"
subdir += ibmpc
endif

ifeq "$(PCE_BUILD_MACPLUS)" "1"
subdir += macplus
endif

ifeq "$(PCE_BUILD_SIM405)" "1"
subdir += sim405
endif

ifeq "$(PCE_BUILD_SIM6502)" "1"
subdir += sim6502
endif

ifeq "$(PCE_BUILD_SIMARM)" "1"
subdir += simarm
endif

ifeq "$(PCE_BUILD_SIMS32)" "1"
subdir += sims32
endif


DCL := Makefile

DIST := Makefile.in


include $(reldir)/rules.inc
