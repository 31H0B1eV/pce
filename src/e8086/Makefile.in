# Makefile
# $Id: Makefile.in,v 1.2 2003/04/17 14:15:06 hampa Exp $

srcdir := @srcdir@
topdir := @top_srcdir@
subdir :=
reldir := ../..

include $(reldir)/Makefile.inc


SRC	:= disasm.c e8086.c ea.c microcode.c util.c

OBJ	:= disasm.o e8086.o ea.o microcode.o util.o

HDR	:= e8086.h internal.h
HDR_EXT	:=

MAN	:=

BIN	:= e8086.a

SDP	:= Makefile $(HDR) $(HDR_EXT)
BDP	:= Makefile $(OBJ) $(OBJ_EXT)


all: $(BIN)


clean:
	rm -f $(BIN) $(OBJ)


e8086.a: $(OBJ)
	rm -f $@
	$(AR) $(AR_FLG) $@ $(OBJ)


disasm.o:	disasm.c $(SDP)
e8086.o:	e8086.c $(SDP)
ea.o:		ea.c $(SDP)
microcode.o:	microcode.c $(SDP)
util.o:		util.c $(SDP)