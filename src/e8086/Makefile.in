# Makefile
# $Id: Makefile.in,v 1.4 2003/08/29 09:45:42 hampa Exp $

srcdir := @srcdir@
topdir := @top_srcdir@
subdir :=
reldir := ../..

include $(reldir)/Makefile.inc


SRC	:= disasm.c e8086.c e80186.c flags.c ea.c microcode.c

OBJ	:= disasm.o e8086.o e80186.o flags.o ea.o microcode.o

HDR	:= e8086.h internal.h
HDR_EXT	:=

MAN	:=

BIN	:= e8086.a

SDP	:= Makefile $(HDR) $(HDR_EXT)
BDP	:= Makefile $(OBJ) $(OBJ_EXT)


all: $(BIN)


clean:
	rm -f $(BIN) $(OBJ)


e8086.a: $(OBJ)
	rm -f $@
	$(AR) $(AR_FLG) $@ $(OBJ)


disasm.o:	disasm.c $(SDP)
e8086.o:	e8086.c $(SDP)
e80186.o:	e80186.c $(SDP)
ea.o:		ea.c $(SDP)
flags.o:	flags.c $(SDP)
microcode.o:	microcode.c $(SDP)
