# Makefile
# $Id$

srcdir := @srcdir@
topdir := @top_srcdir@
subdir := ppc405
reldir := ../..


include $(reldir)/config.inc


SRC := e8237.c e8250.c e8253.c e8255.c e8259.c
OBJ := e8237.o e8250.o e8253.o e8255.o e8259.o
HDR := e8237.h e8250.h e8253.h e8255.h e8259.h
MAN :=
BIN := chipset.a

SDP := Makefile
BDP := Makefile $(OBJ) $(OBJ_EXT)

CLN := $(OBJ) $(BIN)
DCL := Makefile


include $(reldir)/rules.inc


all: $(BIN)


chipset.a: $(OBJ)
	rm -f $@
	$(AR) $(AR_FLG) $@ $(OBJ)


e8237.o:	e8237.c e8237.h $(SDP)
e8250.o:	e8250.c e8250.h $(SDP)
e8253.o:	e8253.c e8253.h $(SDP)
e8255.o:	e8255.c e8255.h $(SDP)
e8259.o:	e8259.c e8259.h $(SDP)
