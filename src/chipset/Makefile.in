# Makefile
# $Id: Makefile.in,v 1.3 2003/09/04 20:11:15 hampa Exp $

srcdir := @srcdir@
topdir := @top_srcdir@
subdir :=
reldir := ../..

include $(reldir)/Makefile.inc


SRC	:= e8250.c e8253.c e8255.c e8259.c

OBJ	:= e8250.o e8253.o e8255.o e8259.o

HDR	:= e8250.h e8253.h e8255.h e8259.h
HDR_EXT	:=

MAN	:=

BIN	:= chipset.a

SDP	:= Makefile
BDP	:= Makefile $(OBJ) $(OBJ_EXT)


all: $(BIN)


clean:
	rm -f $(BIN) $(OBJ)


chipset.a: $(OBJ)
	rm -f $@
	$(AR) $(AR_FLG) $@ $(OBJ)


e8250.o:	e8250.c e8250.h $(SDP)
e8253.o:	e8253.c e8253.h $(SDP)
e8255.o:	e8255.c e8255.h $(SDP)
e8259.o:	e8259.c e8259.h $(SDP)
